{
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "git_panel": {
    "tree_view": true,
  },
  "agent": {
    "default_profile": "write",
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5.2",
    },
    "favorite_models": [],
    "model_parameters": [],
  },
  "features": {
    "edit_prediction_provider": "zed",
  },
  "session": {
    "trust_all_worktrees": true,
  },
  "icon_theme": "Catppuccin Frappé",
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Catppuccin Frappé (Blur)",
  },
  "colorize_brackets": true,
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware",
  },
  "diagnostics": {
    "include_warnings": true,
    "inline": {
      "enabled": true,
    },
  },
  "languages": {
    "Kotlin": {
      "language_servers": ["kotlin-language-server"],
    },
    "Python": {
      "language_servers": ["pyrefly", "ruff"],
    },
  },
  "lsp": {
    "jdtls": {
      "settings": {
        "java_home": "TODO",
        "lombok_support": true,
        "jdk_auto_download": false,
      },
    },
    "kotlin-language-server": {
      "settings": {
        "compiler": {
          "jvm": {
            "target": "17",
          },
        },
      },
      "binary": {
        "env": {
          "JAVA_HOME": "TODO",
        },
      },
    },
  },
  "auto_install_extensions": {
    "html": true,
    "git-firefly": true,
    "latex": true,
    "toml": true,
    "java": true,
    "dart": true,
    "kotlin": true,
    "pyrefly": true,
    "dependi": true,
    "catppuccin-icons": true,
    "catppuccin-blur": true,
    "log": true,
  },
}
